<script>
	function searchnangcao(id) {
		var url = "@Url.Action("Index","SearchAndFilter")?id=" + id;
		window.location.href = url;
	}
	function ThemVaoDanhSachYeuThich(id) {
		var url = "@Url.Action("ThemVaoYeuThich", "Home")?id=" + id;
		$.ajax({
			url: url,
			success: function (data) {
				if (data.success === true) {
					console.log(data.success);
					alert("Đã thêm sản phẩm vào yêu thích");
				} else {
					alert("Sản phẩm này đã có trong mục yêu thích");

				}
			},
			error: function () {
				console.log("AJAX request failed");
			}
		});
	}
	// phiếu giảm giá
	$(document).ready(function () {
		$.ajax({
			url: '@Url.Action("PhieuGiamGia", "Home")',
			type: 'GET',
			success: function (data) {
				console.log(data);
				var listcategory = $("#phieugiam");
				listcategory.empty();
				data.data.forEach(function (item) {
					var categoryRow =
					'<div class="pgg">' +
						'<div class="left">' +
							'<h4>' +item.name +  '</h4>' +
								'<p>' + " Giảm : " + item.percentReduct + "%" + '</p>' +
								'<p>' + "Tối đa : " + formatPrice(item.reductMax) + '</p>' +
								'<p>' + "Áp dụng với hóa đơn từ : " + formatPrice(item.condition) + '</p>' +
						'</div>' +
						'<div class="right">' +
						'<p>' + formatDate(item.startDate) + '</p>' +
						'<p style="font-size: 1.2em;font-weight: 700;color: #F39801; " id="code">' + item.code + '</p>' +
						'<button onclick="copyPromoCode()" >' + "Copy mã" + '</button>' +
						'</div>' +
					'</div>';

					listcategory.append(categoryRow);
				});
			},
			error: function (error) {
				console.log(error);
			}
		});
	});
	function copyPromoCode() {
		var promoCodeElement = document.getElementById("code");
		var promoCodeText = promoCodeElement.textContent;
		var tempElement = document.createElement("textarea");
		document.body.appendChild(tempElement);
		tempElement.value = promoCodeText;
		tempElement.select();
		document.execCommand("copy");
		document.body.removeChild(tempElement);

		alert("Đã sao chép mã khuyến mãi!");
	}
	/*danh mục */
	$(document).ready(function () {
		$.ajax({
			url: '@Url.Action("SachMoi", "Home")',
			type: 'GET',
			success: function (data) {
				console.log(data);
				var listcategory = $("#listcategory");
				listcategory.empty();
				data.data.forEach(function (item) {
					var categoryRow =
